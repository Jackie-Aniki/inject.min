{
  "name": "inject.min",
  "version": "2.0.1",
  "description": "super easy and super small dependency injection - all that you need | no need to register | can override | can use inject | can use decorators",
  "keywords": [
    "typescript",
    "javascript",
    "dependency injection",
    "di",
    "inject",
    "injectable",
    "decorators",
    "inversion of control",
    "ioc",
    "lightweight",
    "zero dependency",
    "typescript library",
    "typescript di",
    "dependency management",
    "override",
    "bind"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "url": "git+ssh://git@github.com/Jackie-Aniki/inject.min.git"
  },
  "author": "Jackie-Aniki <Jacek Pietal> <Jackie-Aniki85@gmail.com>",
  "license": "MIT",
  "private": false,
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "chef-express": "^3.2.3",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typedoc": "^0.28.14",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "format": "prettier . --write",
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "postbuild": "yarn typedoc",
    "docs": "yarn build && chef-express docs --port 3000",
    "test": "yarn jest --verbose",
    "test-ts": "ts-node tests/with-decorators.ts",
    "precommit": "yarn build && yarn format",
    "amend": "yarn precommit && git commit -a --am --no-edit"
  },
  "files": [
    "src/*.*",
    "dist/*.*"
  ]
}
